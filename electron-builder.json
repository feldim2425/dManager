{
  "productName": "dManager",
  "directories": {
    "output": "app-builds"
  },
  "win": {
    "icon": "dist/favicon.ico",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "dist/favicon.icns",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist",
    "target": [
      "AppImage"
    ]
  },
  "appImage": {
    "systemIntegration": "doNotAsk"
  },
  "fileAssociations": [
      {
        "ext": "dmanager",
        "name": "dManager",
        "mimeType": "x-scheme-handler/dmanager"
      }
    ],
  "protocols":[{
    "name":"dmanager",
    "role":"Viewer",
    "schemes": ["dmanager"]
  }]
}
